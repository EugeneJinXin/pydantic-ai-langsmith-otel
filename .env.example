# Environment variables for pydantic-ai with LangSmith tracing

# OpenAI API key for pydantic-ai agent
OPENAI_API_KEY=your_openai_api_key_here

# LangSmith configuration
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=pydantic-ai-demo

# Optional: OpenTelemetry configuration (if using environment variables instead of code)
OTEL_EXPORTER_OTLP_ENDPOINT=https://api.smith.langchain.com/otel
OTEL_EXPORTER_OTLP_HEADERS=x-api-key=your_langsmith_api_key_here

